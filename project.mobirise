                },
                ".nav-item:first-child > .btn.active:before": {
                  "-webkit-transform": "translateX(0)",
                  "transform": "translateX(0)"
                },
                ".nav-item:last-child > .btn.active:before": {
                  "-webkit-transform": "translateX(0)",
                  "transform": "translateX(0)"
                }
              }
            },
            ".mbr-iconfont": {
              "font-size": "60px",
              "color": "@primaryColor"
            }
